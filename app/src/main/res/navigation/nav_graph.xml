<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/acordoFragment">

    <fragment
        android:id="@+id/acordoFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.acordo.AcordoFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_acordo" >
        <action
            android:id="@+id/action_acordoFragment_to_clienteFragment"
            app:destination="@id/clienteFragment" />
    </fragment>
    <fragment
        android:id="@+id/clienteFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.cliente.ClienteFragment"
        android:label="fragment_cliente"
        tools:layout="@layout/fragment_cliente" />
    <fragment
        android:id="@+id/servicosFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.servicos.ServicosFragment"
        android:label="fragment_servicos"
        tools:layout="@layout/fragment_servicos" />
    <fragment
        android:id="@+id/observacaoFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.observacao.ObservacaoFragment"
        android:label="fragment_observacao"
        tools:layout="@layout/fragment_observacao" />
    <fragment
        android:id="@+id/reverConcluirFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.rever.ReverConcluirFragment"
        android:label="fragment_rever_concluir"
        tools:layout="@layout/fragment_rever_concluir" />
    <fragment
        android:id="@+id/opcoesAvancadasFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.definicoes.OpcoesAvancadasFragment"
        android:label="fragment_opcoes_avancadas"
        tools:layout="@layout/fragment_opcoes_avancadas" >
        <action
            android:id="@+id/action_opcoesAvancadasFragment_to_tiposFragment"
            app:destination="@id/tiposFragment" />
    </fragment>
    <fragment
        android:id="@+id/tiposFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.definicoes.TiposFragment"
        android:label="fragment_tipos"
        tools:layout="@layout/fragment_tipos" />
    <fragment
        android:id="@+id/definicoesFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.definicoes.DefinicoesFragment"
        android:label="DefinicoesFragment" />
    <fragment
        android:id="@+id/acordosRealizadosFragment"
        android:name="com.artemissoftware.vvmcontratos.ui.acordo.AcordosRealizadosFragment"
        android:label="AcordosRealizadosFragment" />
</navigation>